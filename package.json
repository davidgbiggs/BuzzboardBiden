    "@react-native-community/eslint-config": "*",
    "babel-jest": "*",
    "babel-plugin-transform-remove-console": "*",
    "eslint": "*",
    "eslint-plugin-react-hooks": "*",
    "jest": "*",
    "metro-react-native-babel-preset": "*",
